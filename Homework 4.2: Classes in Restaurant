import random

dishes_string = ('eggs, Cake, cake, Spam, Spam   ,,, New, new, cookie ')

new_list =[]
dish_set = []

class MyDish():
    '''Dish you ordered in the restaurant'''

    def __init__(self, dish_name):
            self.name = []
            self.name = dish_name
            #print(self.name)
 
    def call_time(self,random_a,random_b):
        '''Randomizer of a time'''
        dish_time = random.randint(random_a,random_b)
        return (dish_time)

    def print_dish(self):
        '''Print what you ordered and time'''
        #for dish in self.name:
        print ("{:.<15}{:.>20} min"
        .format(self.name,self.call_time(0,100)))


for el in dishes_string.title().split(','):
    if el.strip() != "":
        new_list.append(el.strip())
        dish_set = list(set(new_list))
        
for i in range(len(dish_set)):
    time_for_dish = MyDish(dish_set[i])
    time_for_dish.print_dish()


# OUTPUT
#Spam.............................93 min
#New...............................7 min
#Cake.............................20 min
#Eggs.............................82 min
