import random

dishes_string = ('eggs, Cake, cake, Spam, Spam   ,,, New, new, cookie ')

new_list = set(list([item.strip() for item in dishes_string.title().split(',') if item.strip() !=""]))
#print(new_list)

class MyDish():
    '''Dish you ordered in the restaurant'''

    def __init__(self, dish_names):
            self.name = list(dish_names)
            #print(self.name)
 
    def call_time(self,random_a,random_b):
        '''Randomizer of a time'''
        dish_time = random.randint(random_a,random_b)
        return (dish_time)

    def print_dish(self):
        '''Print what you ordered and time'''
        for dish in self.name:
            print ("{:.<15}{:.>20} min"
           .format(dish,self.call_time(0,100)))

new_dish = MyDish(new_list)
new_dish.print_dish()


# OUTPUT
#Spam.............................93 min
#New...............................7 min
#Cake.............................20 min
#Eggs.............................82 min
